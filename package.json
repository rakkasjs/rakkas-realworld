{
  "name": "rakkas-realworld-demo",
  "private": true,
  "type": "module",
  "version": "2.0.0",
  "description": "Rakkas RealWorld demo",
  "scripts": {
    "dev": "dotenv vite dev",
    "build": "prisma generate && rakkas build",
    "build:cloudflare-workers": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate && rakkas build -d cloudflare-workers",
    "build:netlify": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate && rakkas build -d netlify",
    "vercel-build": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate && rakkas build -d vercel",
    "configure": "dotenv node configure.js",
    "start": "dotenv node dist/server",
    "test": "pnpm test:unit && NODE_ENV=test start-server-and-test 'pnpm build && pnpm start' :3000/api/test/ping 'pnpm test:api && pnpm test:e2e'",
    "test:unit": "jest",
    "test:api": "jest --config jest-api-test.config.json --maxWorkers=1 api-test",
    "test:e2e": "cypress run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:ts": "eslint --max-warnings=0 src",
    "lint:css": "stylelint \"src/**/*.css\"",
    "format": "prettier . --write",
    "prepack": "pnpm build"
  },
  "files": [
    "configure.js",
    "dist",
    "prisma"
  ],
  "dependencies": {
    "@hattip/cookie": "^0.0.17",
    "@hattip/response": "^0.0.17",
    "@prisma/client": "^4.1.1",
    "@tsndr/cloudflare-worker-jwt": "^2.0.1",
    "bcrypt": "^5.0.1",
    "cookie": "^0.5.0",
    "dotenv-cli": "^6.0.0",
    "find-free-port": "^2.0.0",
    "http-status-codes": "^2.1.4",
    "jose": "^4.8.3",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^4.1.1",
    "rakkasjs": "next",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "safe-compare": "^1.1.4",
    "slugify": "^1.6.2",
    "validator": "^13.7.0",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@hattip/compose": "^0.0.17",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^13.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^18.6.3",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/safe-compare": "^1.1.0",
    "@types/set-cookie-parser": "^2.4.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/validator": "^13.7.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "core-js": "^3.24.1",
    "cypress": "^10.4.0",
    "dotenv-load": "^2.0.0",
    "esbuild": "^0.14.53",
    "esbuild-jest": "^0.5.0",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.3",
    "jest-css-modules-transform": "^4.3.0",
    "jest-environment-jsdom": "^28.1.3",
    "node-fetch": "^3.2.10",
    "prettier": "^2.7.1",
    "set-cookie-parser": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.9.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.7.4",
    "vite": "3.0.4",
    "vite-tsconfig-paths": "^3.3.17",
    "vitest": "^0.20.3"
  }
}
