{
	"name": "rakkas-realworld-demo",
	"private": true,
	"type": "module",
	"version": "2.0.0",
	"description": "Rakkas RealWorld demo",
	"scripts": {
		"dev": "dotenv vite dev",
		"build": "prisma generate && rakkas build",
		"build:cfw": "prisma generate --data-proxy && RAKKAS_TARGET=cloudflare-workers rakkas build",
		"build:netlify": "prisma generate --data-proxy && RAKKAS_TARGET=netlify rakkas build",
		"build:vercel": "prisma generate --data-proxy && RAKKAS_TARGET=vercel rakkas build",
		"configure": "dotenv node configure.js",
		"start": "dotenv node dist/server",
		"test:unit": "vitest run src",
		"test:api": "vitest run api-test --no-threads",
		"test:e2e": "cypress run",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint:ts": "eslint --max-warnings=0 src",
		"format": "prettier . --write",
		"prepack": "pnpm build"
	},
	"files": [
		"configure.js",
		"dist",
		"prisma"
	],
	"dependencies": {
		"@hattip/cookie": "^0.0.17",
		"@hattip/response": "^0.0.17",
		"@prisma/client": "^4.1.1",
		"bcrypt": "^5.0.1",
		"cookie": "^0.5.0",
		"dotenv-cli": "^6.0.0",
		"find-free-port": "^2.0.0",
		"http-status-codes": "^2.1.4",
		"jose": "^4.8.3",
		"prisma": "^4.1.1",
		"rakkasjs": "next",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-markdown": "^8.0.3",
		"slugify": "^1.6.2",
		"zod": "^3.17.10"
	},
	"devDependencies": {
		"@hattip/adapter-cloudflare-workers": "^0.0.17",
		"@hattip/adapter-netlify-functions": "^0.0.17",
		"@hattip/compose": "^0.0.17",
		"@rakkasjs/eslint-config": "0.6.0-canary.1",
		"@types/bcrypt": "^5.0.0",
		"@types/cookie": "^0.5.1",
		"@types/jsonwebtoken": "^8.5.6",
		"@types/node": "^18.6.3",
		"@types/node-fetch": "^2.6.2",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/set-cookie-parser": "^2.4.1",
		"bcryptjs": "^2.4.3",
		"cypress": "^10.4.0",
		"eslint": "^8.21.0",
		"node-fetch": "^3.2.10",
		"prettier": "^2.7.1",
		"set-cookie-parser": "^2.5.1",
		"typescript": "^4.7.4",
		"vite": "3.0.4",
		"vite-tsconfig-paths": "^3.3.17",
		"vitest": "^0.20.3"
	}
}
